ot;&gt;0&lt;/span&gt;,B=&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;,i,j,z[&lt;span class=&quot;number&quot;&gt;1760&lt;/span&gt;];&lt;span class=&quot;type&quot;&gt;char&lt;/span&gt; b[&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;     &lt;span class=&quot;number&quot;&gt;1760&lt;/span&gt;];&lt;span class=&quot;built_in&quot;&gt;printf&lt;/span&gt;(&lt;span class=&quot;string&quot;&gt;&amp;quot;\x1b[2J&amp;quot;&lt;/span&gt;);&lt;span class=&quot;keyword&quot;&gt;for&lt;/span&gt;(;;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  )&amp;#123;&lt;span class=&quot;built_in&quot;&gt;memset&lt;/span&gt;(b,&lt;span class=&quot;number&quot;&gt;32&lt;/span&gt;,&lt;span class=&quot;number&quot;&gt;1760&lt;/span&gt;);&lt;span class=&quot;built_in&quot;&gt;memset&lt;/span&gt;(z,&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;,&lt;span class=&quot;number&quot;&gt;7040&lt;/span&gt;)&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  ;&lt;span class=&quot;keyword&quot;&gt;for&lt;/span&gt;(j=&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;;&lt;span class=&quot;number&quot;&gt;6.28&lt;/span&gt;&amp;gt;j;j+=&lt;span class=&quot;number&quot;&gt;0.07&lt;/span&gt;)&lt;span class=&quot;keyword&quot;&gt;for&lt;/span&gt;(i=&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;;&lt;span class=&quot;number&quot;&gt;6.28&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; &amp;gt;i;i+=&lt;span class=&quot;number&quot;&gt;0.02&lt;/span&gt;)&amp;#123;&lt;span class=&quot;type&quot;&gt;float&lt;/span&gt; c=&lt;span class=&quot;built_in&quot;&gt;sin&lt;/span&gt;(i),d=&lt;span class=&quot;built_in&quot;&gt;cos&lt;/span&gt;(j),e=&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; &lt;span class=&quot;built_in&quot;&gt;sin&lt;/span&gt;(A),f=&lt;span class=&quot;built_in&quot;&gt;sin&lt;/span&gt;(j),g=&lt;span class=&quot;built_in&quot;&gt;cos&lt;/span&gt;(A),h=d+&lt;span class=&quot;number&quot;&gt;2&lt;/span&gt;,D=&lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;/(c*&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; h*e+f*g+&lt;span class=&quot;number&quot;&gt;5&lt;/span&gt;),l=&lt;span class=&quot;built_in&quot;&gt;cos&lt;/span&gt;      (i),m=&lt;span class=&quot;built_in&quot;&gt;cos&lt;/span&gt;(B),n=s\&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;in(B),t=c*h*g-f*        e;&lt;span class=&quot;type&quot;&gt;int&lt;/span&gt; x=&lt;span class=&quot;number&quot;&gt;40&lt;/span&gt;+&lt;span class=&quot;number&quot;&gt;30&lt;/span&gt;*D*&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;(l*h*m-t*n),y=            &lt;span class=&quot;number&quot;&gt;12&lt;/span&gt;+&lt;span class=&quot;number&quot;&gt;15&lt;/span&gt;*D*(l*h*n&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;+t*m),o=x+&lt;span class=&quot;number&quot;&gt;80&lt;/span&gt;*y,          N=&lt;span class=&quot;number&quot;&gt;8&lt;/span&gt;*((f*e-c*d*g&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; )*m-c*d*e-f*g-l        *d*n);&lt;span class=&quot;keyword&quot;&gt;if&lt;/span&gt;(&lt;span class=&quot;number&quot;&gt;22&lt;/span&gt;&amp;gt;y&amp;amp;&amp;amp;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; y&amp;gt;&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;&amp;amp;&amp;amp;x&amp;gt;&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;&amp;amp;&amp;amp;&lt;span class=&quot;number&quot;&gt;80&lt;/span&gt;&amp;gt;x&amp;amp;&amp;amp;D&amp;gt;z[o])&amp;#123;z[o]=D;;;b[o]=&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt; &lt;span class=&quot;string&quot;&gt;&amp;quot;.,-~:;=!*#$@&amp;quot;&lt;/span&gt;[N&amp;gt;&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;?N:&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;];&amp;#125;&amp;#125;&lt;span class=&quot;comment&quot;&gt;/*#****!!-*/&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;built_in&quot;&gt;printf&lt;/span&gt;(&lt;span class=&quot;string&quot;&gt;&amp;quot;\x1b[H&amp;quot;&lt;/span&gt;);&lt;span class=&quot;keyword&quot;&gt;for&lt;/span&gt;(k=&lt;span class=&quot;number&quot;&gt;0&lt;/span&gt;;&lt;span class=&quot;number&quot;&gt;1761&lt;/span&gt;&amp;gt;k;k++)&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;   &lt;span class=&quot;built_in&quot;&gt;putchar&lt;/span&gt;(k%&lt;span class=&quot;number&quot;&gt;80&lt;/span&gt;?b[k]:&lt;span class=&quot;number&quot;&gt;10&lt;/span&gt;);A+=&lt;span class=&quot;number&quot;&gt;0.04&lt;/span&gt;;B+=&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;     &lt;span class=&quot;number&quot;&gt;0.02&lt;/span&gt;;&amp;#125;&amp;#125;&lt;span class=&quot;comment&quot;&gt;/*****####*******!!=;:~&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;       ~::==!!!**********!!!==::-&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;         .,~~;;;========;;;:~-.&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;             ..,--------,*/&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;

&lt;p&gt;&lt;code&gt;gcc -o donut donut.c -lm &amp;amp;&amp;amp; ./donut&lt;/code&gt;&lt;/p&gt;
